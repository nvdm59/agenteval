# AgentEval Configuration
# Copy this file to .env and fill in your values

# General Settings
AGENTEVAL_LOG_LEVEL=INFO
AGENTEVAL_CACHE_DIR=.agenteval_cache
AGENTEVAL_TRACE_DIR=.agenteval_traces

# Execution Settings
AGENTEVAL_DEFAULT_EXECUTOR=sequential
AGENTEVAL_MAX_CONCURRENCY=5
AGENTEVAL_TASK_TIMEOUT=300

# API Keys
# Anthropic (Claude)
AGENTEVAL_ANTHROPIC_API_KEY=your_anthropic_key_here

# OpenAI (GPT)
AGENTEVAL_OPENAI_API_KEY=your_openai_key_here

# Google (Gemini)
AGENTEVAL_GOOGLE_API_KEY=your_google_key_here

# Metrics Settings
AGENTEVAL_ENABLE_LLM_JUDGE=false
AGENTEVAL_JUDGE_MODEL=gpt-4

# Reporting Settings
AGENTEVAL_DEFAULT_FORMAT=console
AGENTEVAL_SAVE_TRACES=true

# Cost Tracking
AGENTEVAL_TRACK_COSTS=true
